{{!
    @template tool_admin_presets/not_applicable_settings

    Template for non existing settings in current version.

    Example context (json):
    {
        "skippedchanges": {
            "show": 1,
            "caption": "Applied settings",
            "settings": [
                {
                    "plugin": "quiz",
                    "visiblename": "Decimal places in grades",
                    "oldvisiblevalue": "0",
                    "visiblevalue": "2"
                }
            ]
        }
    }
}}

{{#skippedchanges.show}}
    <h3 class="admin_presets_error">{{#str}}settingsnotapplicable, tool_admin_presets{{/str}}</h3>
    {{#skippedchanges}}{{> tool_admin_presets/local/tables/actual_value_settings_table}}{{/skippedchanges}}
{{/skippedchanges.show}}

