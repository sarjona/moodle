{{!
    @template tool_admin_presets/settings_application

    Template for preset loading result.

    Example context (json):
    {
        "nopresets": 0,
        "showactions": 1,
    }
}}

{{#appliedchanges.show}}
    <h3 class="admin_presets_success">{{#str}}settingsapplied, tool_admin_presets{{/str}}</h3>
    {{#appliedchanges}}{{> tool_admin_presets/local/tables/old_and_new_value_settings_table}}{{/appliedchanges}}
{{/appliedchanges.show}}
{{^appliedchanges.show}}
    <h3 class="admin_presets_error">{{#str}}nothingloaded, tool_admin_presets{{/str}}</h3>
{{/appliedchanges.show}}
{{#skippedchanges.show}}
    <h3>{{#str}}settingsnotapplied, tool_admin_presets{{/str}}</h3>
    {{#skippedchanges}}{{> tool_admin_presets/local/tables/actual_value_settings_table}}{{/skippedchanges}}
{{/skippedchanges.show}}
